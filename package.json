{
  "name": "vue3-vite7-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 20.19.0",
    "pnpm": ">= 8"
  },
  "scripts": {
    "serve": "vite --mode development",
    "build:test": "VITE_CJS_IGNORE_WARNING=true vite build --mode test",
    "build:production": "VITE_CJS_IGNORE_WARNING=true vite build --mode production",
    "lint": "eslint \"src/**/*.{vue,js}\" --fix",
    "eslint:fix": "eslint \"src/**/*.{vue,js}\" --fix",
    "prettier:fix": "prettier --write \"src/**/*.{js,vue,scss,css,json,html}\"",
    "stylelint:fix": "stylelint \"**/*.{html,vue,css,scss}\" --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run",
    "release:changelog": "release-it --only-changelog",
    "release:github": "release-it --ci --github.release",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vueuse/components": "14.0.0",
    "@vueuse/core": "14.0.0",
    "axios": "1.8.4",
    "dayjs": "1.11.19",
    "element-plus": "2.9.11",
    "gsap": "3.13.0",
    "js-md5": "0.8.3",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "nprogress": "0.2.0",
    "pinia": "2.2.8",
    "pinia-plugin-persistedstate": "3.2.3",
    "terser": "5.36.0",
    "unplugin-auto-import": "0.19.0",
    "unplugin-element-plus": "0.9.1",
    "unplugin-icons": "0.19.3",
    "unplugin-vue-components": "0.28.0",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-inspect": "11.3.3",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "4.0.0",
    "vue": "3.5.26",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@release-it/conventional-changelog": "10.0.4",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "@eslint/js": "9.20.0",
    "@vitejs/plugin-legacy": "7.2.1",
    "@vitejs/plugin-vue": "6.0.3",
    "autoprefixer": "10.4.20",
    "eslint": "9.30.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-vue": "9.27.0",
    "globals": "15.11.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "postcss": "8.4.33",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.6.2",
    "release-it": "19.2.4",
    "sass": "1.81.0",
    "stylelint": "16.8.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recess-order": "4.6.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.0",
    "vite": "7.3.1",
    "vue-eslint-parser": "10.2.0"
  }
}
